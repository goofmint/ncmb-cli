#!/usr/bin/env node
var NCMB = require("ncmb");
var fs = require('fs');
var expandHomeDir = require('expand-home-dir')
var program = require('commander');

program
  .version('0.0.1')
  .command('init [application_key client_key]', 'Save application_key and client_key')
  .command('script [filename]', 'Execute mBaaS script.')
  .command('datastore [args]', 'Manage data in DataStore')
  .command('csv [args]', 'Import or convert CSV file')
  .parse(process.argv);


